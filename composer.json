{
    "name": "aaronfrancis/flaky",
    "description": "A Laravel package to elegantly handle flaky operations.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aaron@tryhardstudios.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10|^11.0|^12.0",
        "illuminate/cache": "^10|^11.0|^12.0",
        "illuminate/console": "^10|^11.|^12.00"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "phpunit/phpunit": ">=8.5.23|^9|^10",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AaronFrancis\\Flaky\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AaronFrancis\\Flaky\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronFrancis\\Flaky\\Providers\\FlakyServiceProvider"
            ]
        }
    }
}
